namespace For
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("quantos numero ser√£o digitados?");
            int n = int.Parse(Console.ReadLine());
            int soma = 0;
            for( int i= 1;i <= n; i++)
            {
                int valor = int.Parse(Console.ReadLine());
                soma += valor; 
            }
            Console.WriteLine("soma = " + soma);
        }
    }
}
